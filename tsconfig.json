{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.tsx"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "types": [
      "element-plus/global",
      "vite/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "suppressImplicitAnyIndexErrors": true
  },
  "noImplicitAny": false,
  "exclude": [
    "node_modules",
  ],
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}